    "version": "0.2.0",
    "configurations": [
        {
            "name": "github-config organization sync",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "organization",
                "sync",
                "-t",
                "${env:GITHUB_OAUTH}",
                "-o",
                "Continuous-X",
                "--config",
                ".github-config.yaml"
            ],
            "env": {
                "github.enterpriseDomain": "",
                "export.github.enterpriseDomain": "",
                "export.github.token": "${env:GITHUB_OAUTH}",
                "export.github.organization": "Continuous-X",
                "export.github.repository": "github-config-backup"
            }
